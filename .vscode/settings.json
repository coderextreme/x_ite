{
   "files.exclude": {
      "**/*~": true,
      "node_modules": true,
      "package-lock.json": true,
      "docs/_site": true,
      "docs/.jekyll-cache": true
   },
   "files.insertFinalNewline": true,
   "files.trimFinalNewlines": true,
   "files.trimTrailingWhitespace": true,
   "files.eol": "\n",
